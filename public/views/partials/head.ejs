  <body>

  <% if(locals.success_messages){ %>
      <script>
        Swal.fire({
          icon: 'success',
          title: 'Oops...',
          text: 'Something went wrong!',
          footer: '<a href="">Why do I have this issue?</a>'
        })
      </script>
  <% } %>
  <% if(locals.error_messages){ %>
      <script>
        Swal.fire({
          icon: 'error',
          title:  locals.error_messages.title ,
          text: locals.error_messages.description,
        })
      </script>
  <% } %>
